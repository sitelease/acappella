{
  "name": "bricev/compolab",
  "description": "Composer Repository for GitLab",
  "license": "proprietary",
  "require": {
    "sitelease/gitea-php": "@dev",
    "php-http/guzzle6-adapter": "^1.0",
    "symfony/config": "^4.0",
    "symfony/console": "^4.0",
    "symfony/dependency-injection": "^4.0",
    "symfony/event-dispatcher": "^4.0",
    "symfony/http-kernel": "^4.0",
    "symfony/routing": "^4.0",
    "symfony/yaml": "^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6"
  },
  "autoload": {
    "psr-4": {
      "CompoLab\\": "src",
      "CompoLab\\Tests\\": "tests/src"
    }
  },
  "repositories": {
    "local": {
      "type": "path",
      "url": "../gitea.php/",
      "options": {
        "symlink": true
      }
    }
  }
}
